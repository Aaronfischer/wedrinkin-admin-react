    "build": "npm-run-all --parallel build:**",
    "build:app": "npm-run-all build-css build-js",
    "build:functions": "netlify-lambda build functions",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
